
# aliases for blue green demo

alias clean="gradlew :server:flywayClean -i"
alias migrate="gradlew :server:flywayMigrate -i"

alias blueClient="gradlew :perf:bootRun --args='--connect.port=9001'"
alias blueDeploy="gradlew :server:bootRun --args='--server.port=9001'"

alias greenClient="gradlew :perf:bootRun --args='--connect.port=9002'"
alias greenDeploy="gradlew :server:bootRun --args='--server.port=9002'"
